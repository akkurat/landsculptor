{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": false,
    "sourceMap": true,
    "module": "es2020",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "strict": false,
    "typeRoots": [
      "./node_modules/@types",
        "./types/",
    ] ,
    "alwaysStrict": false,
    "lib": ["ES6", "DOM","DOM.Iterable","ScriptHost","ES2019"]

  },
}
